    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}symfony!{% endblock %}</title>
    <link href="{{ asset('build/css/app.css') }}" rel="stylesheet">
    {% block stylesheets %}{% endblock %}
</head>
<body class="h-100">
